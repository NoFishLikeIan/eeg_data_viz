{
  "name": "eeg_data_viz",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache build out",
    "build": "yarn clean && parcel build index.html -d out --public-url ./ --no-source-maps --no-cache --experimental-scope-hoisting --detailed-report",
    "start": "parcel index.html -p 8080 --open"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "terser": "^4.3.8",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@thi.ng/compose": "latest",
    "@thi.ng/geom": "latest",
    "@thi.ng/hdom": "latest",
    "@thi.ng/hdom-canvas": "latest",
    "@thi.ng/math": "latest",
    "@thi.ng/vectors": "latest"
  },
  "browserslist": [
    "last 3 Chrome versions"
  ],
  "browser": {
    "process": false
  }
}